<%= form_with(model: @user, url: users_path) do |form| %>
  <%= form.label 'Nome' %>
  <%= form.text_field :name %><br>

  <%= form.label 'Email' %>
  <%= form.email_field :email %><br>

  <%= form.label 'Senha' %>
  <%= form.password_field :password %><br>

  <%= form.label 'Confirmação de senha' %>
  <%= form.password_field :password_confirmation %><br>

  <%= form.label :grade_level %>
  <%= form.select :grade_level, User.grade_levels.keys %><br>

  <%= form.submit "Criar Usuário" %>
<% end %>